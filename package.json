{
  "private": true,
  "name": "recom",
  "description": "",
  "license": "MIT",
  "scripts": {
    "build:remix": "remix build",
    "build:css": "tailwindcss  -o ./app/styles/tailwind-prod.css --minify",
    "build": "npm-run-all migrate:deploy build:css build:remix",
    "postinstall": "remix setup node",
    "migrate:deploy": "prisma migrate deploy",
    "dev:remix": "remix dev",
    "dev:css": "tailwindcss -i ./app/styles/tailwind.css -o ./app/styles/tailwind-prod.css -w",
    "dev": "npm-run-all --parallel dev:*",
    "clean": "rimraf .cache ./api/build ./public/build ./app/styles/tailwind-prod.css",
    "studio": "prisma studio"
  },
  "dependencies": {
    "@headlessui/react": "^1.4.2",
    "@heroicons/react": "^1.0.5",
    "@prisma/client": "^3.7.0",
    "@remix-run/react": "^1.1.1",
    "@remix-run/serve": "^1.1.1",
    "@remix-run/vercel": "^1.1.1",
    "bcryptjs": "^2.4.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.1.1",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.1.1",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "autoprefixer": "^10.4.0",
    "daisyui": "^1.20.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.5",
    "prisma": "^3.7.0",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.0.8",
    "tailwindcss-debug-screens": "^2.2.1",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
